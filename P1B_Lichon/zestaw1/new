(defun my-max (L)
        (cond
                ((null L) nil)
                ((and (numberp (car L)) (numberp (my-max (cdr L)))) (max (car L) (my-max (cdr L))))
                ((numberp (car L)) (car L))
                (T (my-max (cdr L)))
        )
)
(defun klucz (L)
	(mapcar 
		#'(lambda (A)(list (append '(nazwisko) (last A))(append '(imie) (butlast A))))
		L
	)
)
